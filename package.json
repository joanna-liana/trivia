{
  "name": "trivia",
  "version": "1.0.0",
  "description": "Trivia kata in typescript",
  "main": "src/game-runner.ts",
  "scripts": {
    "start": "ts-node src/game-runner.ts",
    "test": "nyc mocha --recursive --require ts-node/register \"tests/**/*.test.ts\""
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "typings"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "text",
      "html"
    ]
  },
  "author": "Felix Blechschmitt",
  "license": "GPL-3.0",
  "dependencies": {
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/sinon": "^10.0.13",
    "chai": "^4.1.2",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "sinon": "^15.0.0",
    "ts-node": "^7.0.0"
  }
}
